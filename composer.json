{
    "name": "pigvelop/lumen-application",
    "description": "Application for Lumen 5.5",
    "license": "MIT",
    "authors": [
        {
            "name": "Caique Bertolozzi",
            "email": "caiquebb@gmail.com",
            "homepage": "https://github.com/caiquebb"
        }
    ],
    "homepage": "https://github.com/pigvelop/lumen-application",
    "keywords": ["Laravel", "Lumen", "Application"],
    "require": {
        "illuminate/console": "~5",
        "illuminate/encryption": "~5",
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Pigvelop\\LumenApplication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pigvelop\\LumenApplication\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pigvelop\\LumenApplication\\LumenApplicationServiceProvider"
            ],
            "aliases": {
                "LumenApplication": "Pigvelop\\LumenApplication\\Facades\\LumenApplication"
            }
        }
    }
}
